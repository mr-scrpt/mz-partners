<section class="section page__section-m">
	<div class="section__inner">
		<div class="layout__wrap">

			<div class="section__title title">
				<div class="title__main">Історія компанії</div>
				<div class="title__deco"></div>
			</div>


		</div>
		<div class="layout__cell">
			<div class="history">

				<div class="history__inner">
					<div class="history__textbox">
						<div class="history__text">
							{9 | resource: 'history_text'}
						</div>
					</div>
					<div class="history__imgbox">
						<img
							src="{9 | resource: 'history_img'}"
							alt=""
							class="history__img img"
						>
					</div>
				</div>


			</div>
		</div>
	</div>
</section>